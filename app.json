{
  "expo": {
    "scheme": "in-my-pan",
    "userInterfaceStyle": "dark",
    "orientation": "portrait",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-secure-store",
      "expo-font",
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./src/assets/images/splash-icon-light.png",
          "dark": {
            "image": "./src/assets/images/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends and upload them to your profile.",
          "cameraPermission": "The app accesses your camera to let you take photos and upload them to your profile.",
          "microphonePermission": "The app accesses your microphone to let you record audio and upload them to your profile."
        }
      ]
    ],
    "name": "InMyPan",
    "slug": "InMyPan",
    "description": "In My Pan is a social media app that allows you to share your photos and videos of your cooking with your friends and family.",
    "owner": "godsonumoren",
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "8c50719d-3a92-4842-a023-1142b2c47574"
      }
    },
    "ios": {
      "bundleIdentifier": "com.godsonumoren.InMyPan",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": {
        "dark": "./src/assets/images/ios-dark.png",
        "light": "./src/assets/images/ios-light.png",
        "tinted": "./src/assets/images/ios-tinted.png"
      },
      "usesAppleSignIn": true,
      "runtimeVersion": "1.0.0"
    },
    "updates": {
      "url": "https://u.expo.dev/8c50719d-3a92-4842-a023-1142b2c47574"
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    }
  }
}
